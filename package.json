{
  "name": "nodejs-aws-shop-backend",
  "version": "0.1.0",
  "bin": {
    "nodejs-aws-shop-backend": "bin/nodejs-aws-shop-backend.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:w": "jest --watch",
    "cdk": "cdk",
    "bundle": "webpack",
    "deploy": "npm run bundle && cdk synth && cdk deploy",
    "destroy": "cdk destroy -f",
    "create-bd:all": "ts-node  scripts/create_ProductsDB.ts && ts-node  scripts/create_StocksDB.ts",
    "fill:bd": "ts-node  scripts/filByMockData.ts"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "20.14.2",
    "@types/uuid": "^10.0.0",
    "aws-cdk": "2.146.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.5",
    "webpack": "^5.92.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.602.0",
    "@aws-sdk/lib-dynamodb": "^3.602.0",
    "@aws-sdk/util-dynamodb": "^3.602.0",
    "@aws-solutions-constructs/aws-lambda-dynamodb": "^2.60.0",
    "@types/aws-lambda": "^8.10.138",
    "aws-cdk-lib": "2.146.0",
    "clean-webpack-plugin": "^4.0.0",
    "constructs": "^10.0.0",
    "dotenv": "^16.4.5",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.1",
    "uuid": "^10.0.0"
  }
}
